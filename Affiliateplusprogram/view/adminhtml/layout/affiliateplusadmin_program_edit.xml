<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="admin-2columns-left">
    <update handle="editor"/>
    <body>
        <referenceContainer name="page.main.actions">
            <block class="Magento\Backend\Block\Store\Switcher" name="category.store.switcher" template="Magento_Backend::store/switcher.phtml" />
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magestore\Affiliateplusprogram\Block\Adminhtml\Program\Edit" name="admin.block.program.form.container"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Magestore\Affiliateplusprogram\Block\Adminhtml\Program\Edit\Tabs" name="program_edit_tab">
                <block class="Magestore\Affiliateplusprogram\Block\Adminhtml\Program\Edit\Tab\Form" name="program_edit_tab_form"/>
                <block class="Magestore\Affiliateplusprogram\Block\Adminhtml\Program\Edit\Tab\Conditions" name="program_catalog_edit_tab_conditions"/>
                <block class="Magestore\Affiliateplusprogram\Block\Adminhtml\Program\Edit\Tab\Actions" name="program_edit_tab_actions"/>
            </block>
        </referenceContainer>
    </body>
</page>
